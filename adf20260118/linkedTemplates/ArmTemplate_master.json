{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf20260118"
		},
		"ADLS_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_LS'"
		},
		"ADLS_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage2026018.dfs.core.windows.net/"
		},
		"AmazonS3_LS_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault1",
					"type": "LinkedServiceReference"
				},
				"secretName": "s3accesskeyid"
			}
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvault20260118.vault.azure.net/"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver202601182103.database.windows.net"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb20260118"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"STORAGE_TRIGGER_FOR_JSON_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/80772315-1636-4170-a04c-37ef241980e5/resourceGroups/resourcegroup2612250747/providers/Microsoft.Storage/storageAccounts/storage2026018"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf20260118_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_LS_accountKey": {
						"value": "[parameters('ADLS_LS_accountKey')]"
					},
					"ADLS_LS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_LS_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AmazonS3_LS_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS3_LS_properties_typeProperties_accessKeyId')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_userName')]"
					},
					"STORAGE_TRIGGER_FOR_JSON_properties_typeProperties_scope": {
						"value": "[parameters('STORAGE_TRIGGER_FOR_JSON_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}